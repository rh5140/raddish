  listen   8080;

  root /;

  location / echo {
  }

  location /echo echo {
  }

  location /images/ static {
    root /usr/src/projects/raddish/static_files;
  }

  location /text/ static {
    root /usr/src/projects/raddish/static_files;
  }

  location /applications/ static {
    root /usr/src/projects/raddish/static_files;
  }

  location /tests/static_files/ static {
    root /usr/src/projects/raddish;
  }

}


